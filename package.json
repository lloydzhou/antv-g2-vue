{
  "name": "g2v",
  "version": "1.0.0",
  "description": "Vue component wrapper for antv@g2",
  "repository": {
    "type": "git",
    "url": "https://github.com/lloydzhou/antv-g2-vue.git"
  },
  "author": "lloydzhou",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "typescript",
    "tsx",
    "antv",
    "g2"
  ],
  "files": [
    "lib"
  ],
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "lib": "tsc src/index.ts --esModuleInterop --skipLibCheck --allowSyntheticDefaultImports -t es5 -d --outDir lib",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antv/g2": "^5.1.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
